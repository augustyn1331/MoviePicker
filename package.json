    "react-dom": "^17.0.2",
    "react-test-renderer": "17.0.1",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/src/setupTests.ts",
      "<rootDir>/node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!react-native|@react-native|@react-navigation|node_modules/(?!((jest-)?react-native(-.*)?|@react-native(-community)?)/))"
    ],
    "testPathIgnorePatterns": [
      "/__utils__/"
    ]
  }
